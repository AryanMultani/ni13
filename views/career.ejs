    <script>
        function escapeHtml(unsafe) {
            return unsafe
                .replace(/&/g, "&amp;")
                .replace(/</g, "&lt;")
                .replace(/>/g, "&gt;")
                .replace(/"/g, "&quot;")
                .replace(/'/g, "&#039;");
        }
        function save_enquiry() {
            var honeypot_field = escapeHtml($('#honeypot_field').val());

            var full_name = escapeHtml($("#name").val());

            var email = escapeHtml($("#mailid").val());

            var phone_no = escapeHtml(document.getElementById("phone_no").value);
            var projecttype = escapeHtml($("#projecttype").val());
            var country_Code = escapeHtml($("#country_Code").val());

            var location = document.getElementById("location").value;
            var budget = escapeHtml(document.getElementById("budget").value);

            var message1 = escapeHtml(document.getElementById("message1").value);
            var prev_url = document.getElementById("prev_url").value;
            var page_url = document.getElementById("page_url").value;

            $.ajax
                ({
                    type: 'post',
                    url: "https://psgroup.in/global_enquiry_save",
                    data: {
                        honeypot_field: honeypot_field,
                        full_name: full_name,
                        email: email,
                        projecttype: projecttype,
                        phone_no: phone_no,
                        country_Code: country_Code,
                        location: location,
                        budget: budget,
                        message1: message1,
                        utm_source: "",
                        utm_medium: "",
                        utm_campaign: "",
                        prev_url: prev_url,
                        page_url: page_url

                    },
                    beforeSend: function () {
                        $("#mybutton").prop('disabled', true);
                        $("#mybutton").html('Submitting...');
                    },
                    success: function (response) {

                        // console.log(response);
                        /*window.location.reload();
                             //alert(response);
                         document.getElementById("your_name").value="";
                           document.getElementById("email_phone1").value="";
                         document.getElementById("phone_no2").value="";
                           document.getElementById("nature_project").value="";
                           document.getElementById("message-3").value="";
                        //document.getElementById("message_status").innerHTML="Thank You For Contacting us<br/><br/>";
                        /*document.getElementById("text-count3").innerHTML="120";
                        //return false;*/
                        if (honeypot_field == '') {

                            window.location = "https://psgroup.in/general_thank_you";
                            //window.location.reload();
                        } else {
                            window.location = "https://psgroup.in/";
                        }
                    }
                });


        }

        function resendOTP() {
            var mobile = $("#phone_no").val();
            if (mobile != "") {
                if (mobile.length == 10) {
                    var msg = "";
                    var otp = generateOTP(6);
                    msg += "Your OTP is " + otp;
                    setCookie("otp", otp, 300);
                    //Cookies.set('otp', otp, {expiry : 300});
                    var response = httpGet("https://api.textlocal.in/send/?apiKey=5IjEk5L6b+8-L5oVQWkI447LBwf1CVsWKyluJMJHYv&sender=PSGRUP&numbers=" + mobile + "&message=" + msg);
                    //alert(response);
                    var obj = JSON.parse(response);

                    var status = obj.status;
                    if (status == "success") {

                        $("#div_otp").css("display", "block");
                        //enableResendOTP();
                        $("#otp").focus();
                        $("#status_message").css("display", "none");
                        /*$("#status_message").css("color","green");*/
                        $("#status_message").html("");
                    }
                    else {
                        if (typeof obj.warnings === 'undefined' || obj.warnings === null) {
                            var message = obj.errors[0].message;
                        }
                        else if (obj.warnings[0].message) {
                            var message = obj.warnings[0].message;
                        }

                        $("#div_otp").css("display", "none");
                        $("#status_message").css("color", "red");
                        $("#status_message").html(message);
                        $("#mybutton").addClass("disabled");
                    }
                }
            }
            $("#resend_div").hide();

        }
        function sendOTP(event) {
            var mobile = $("#phone_no").val();

            if (mobile != "") {
                if (mobile.length == 10) {
                    var msg = "";
                    var otp = generateOTP(6);
                    msg += "Your OTP is " + otp;
                    setCookie("otp", otp, 300);
                    //Cookies.set('otp', otp, {expiry : 300});
                    var response = httpGet("https://api.textlocal.in/send/?apiKey=5IjEk5L6b+8-L5oVQWkI447LBwf1CVsWKyluJMJHYv&sender=PSGRUP&numbers=" + mobile + "&message=" + msg);
                    //alert(response);
                    var obj = JSON.parse(response);

                    var status = obj.status;
                    if (status == "success") {

                        $("#div_otp").css("display", "block");
                        //enableResendOTP();
                        $("#otp").focus();
                        $("#status_message").css("display", "none");
                        /*$("#status_message").css("color","green");*/
                        $("#status_message").html("");
                    }
                    else {
                        if (typeof obj.warnings === 'undefined' || obj.warnings === null) {
                            var message = obj.errors[0].message;
                        }
                        else if (obj.warnings[0].message) {
                            var message = obj.warnings[0].message;
                        }

                        $("#div_otp").css("display", "none");
                        $("#status_message").css("color", "red");
                        $("#status_message").html(message);
                        $("#mybutton").addClass("disabled");
                    }
                }
            }
        }
        function httpGet(theUrl) {
            var xmlHttp = new XMLHttpRequest();
            xmlHttp.open("GET", theUrl, false);
            xmlHttp.send(null);
            return xmlHttp.responseText;
        }
        function generateOTP(n) {
            // Declare a digits variable  
            // which stores all digits 
            var digits = '0123456789';
            let OTP = '';
            for (let i = 0; i < n; i++) {
                OTP += digits[Math.floor(Math.random() * 10)];
            }
            return OTP;
        }
        function verifyOTP() {
            var otp = $("#otp").val();
            var saved_otp = getCookie("otp");
            //console.log(saved_otp);
            if (otp.length == 6) {
                if (otp == saved_otp) {
                    $("#otp_error_status").css("color", "green");
                    $("#otp_error_status").html("OTP Verified ");
                    $("#resend_otp").css("display", "none");
                    $("#mybutton").prop('disabled', false);
                    //$("#mybutton").removeClass("disabled");
                }
                else {
                    $("#resend_otp").css("display", "block");
                    $("#otp_error_status").select();
                    $("#otp_error_status").css("color", "red");
                    $("#otp_error_status").html("Invalid OTP ");
                    $("#mybutton").addClass("disabled");

                }
            }
            else {
                $("#mybutton").prop('disabled', true);
                $("#mybutton").addClass("disabled");
            }
        }
        function setCookie(name, value, days) {
            /*var expires = "";
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days*24*60*60*1000));
                expires = "; expires=" + date.toUTCString();
            }*/
            document.cookie = name + "=" + (value || "") + "; path=/";
        }
        function getCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        }
        function eraseCookie(name) {
            document.cookie = name + '=; Max-Age=-99999999;';
        }
        function enable() {
            setTimeout(function () { $("#resend_div").show(); }, 30000);
        }

    </script>
