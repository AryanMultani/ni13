        function validateFileType(input) {
            const allowedMimeTypes = [
                'application/pdf',
                'application/msword',
                'application/vnd.ms-office',
                'application/vnd.openxmlformats-officedocument.wordprocessingml.document'
            ];

            const allowedExtensions = ['pdf', 'doc', 'docx'];

            const errorMsg = document.getElementById('error-msg');
            const submitBtn = document.getElementById('wait'); // assuming your submit button has an id 'wait'

            if (input.files && input.files[0]) {
                const file = input.files[0];
                const fileName = file.name;
                const fileExtension = fileName.split('.').pop().toLowerCase();
                const fileReader = new FileReader();

                fileReader.onloadend = function () {
                    const arr = (new Uint8Array(this.result)).subarray(0, 4);
                    let header = "";
                    for (let i = 0; i < arr.length; i++) {
                        header += arr[i].toString(16);
                    }

                    let mimeType;
                    switch (header) {
                        case "25504446":
                            mimeType = "application/pdf";
                            break;
                        case "d0cf11e0":
                            mimeType = "application/msword";
                            break;
                        case "504b34":
                        case "504b0304":
                            mimeType = "application/vnd.openxmlformats-officedocument.wordprocessingml.document";
                            break;
                        default:
                            mimeType = "unknown";
                            break;
                    }

                    if (!allowedMimeTypes.includes(mimeType) || !allowedExtensions.includes(fileExtension)) {
                        errorMsg.style.display = 'block';
                        submitBtn.disabled = true; // disable the submit button
                        input.value = ''; // Clear the file input to allow a valid upload
                    } else {
                        errorMsg.style.display = 'none';
                        submitBtn.disabled = false; // enable the submit button
                    }
                };

                fileReader.readAsArrayBuffer(file);
            }
        }
    </script>
